nome = input("qual e o seu nome? ")
peso = float(input(" qual e o seu peso? "))
altura = float(input("qual e o sua altura? "))

imc = peso /(altura*altura) 
 
if imc <=18.5:
    print(f"{nome} abaixo do peso")

elif imc <=24.9:
    print(F"{nome} peso do normal") 

elif imc <=29.9:
    print(f"{nome} obsidade grau I") 

elif imc <=34.9:
    print(f"{nome} obesidade grau II")

else:
    print(f"{nome}esta com obesidade grau III(morbida!!! ta pesado em)e com o imc:")         
